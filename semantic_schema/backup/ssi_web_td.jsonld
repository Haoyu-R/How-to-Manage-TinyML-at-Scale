{
  "@context": [
    "https://www.w3.org/2019/wot/td/v1",
    {
      "sosa": "http://www.w3.org/ns/sosa/",
      "ssn": "http://www.w3.org/ns/ssn/",
      "ssn-system": "http://www.w3.org/ns/ssn/systems",
      "s3n": "http://w3id.org/s3n/",
      "om": "http://www.ontology-of-units-of-measure.org/resource/om-2/",
      "schema": "http://schema.org/",
      "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
      "demo": "http://ureasoner.org/tinyml-demo#",
      "@language": "en"
    }
  ],
  "@type": [
    "s3n:SmartSensor"
  ],
  "title": "SSI-Web_1",
  "description": "SIEMENS SSI-Web Node",
  "ssn:hasSubSystem": [
    {
      "@type": "s3n:MicroController",
      "rdfs:comment": "SIEMENS SSI-Web Node Micro Controller.",
      "s3n:hasSystemCapability": [
        {
          "ssn-system:hasSystemProperty": {
            "@type": "s3n:Memory",
            "schema:value": 512,
            "schema:unitCode": "om:kilobyte",
			"rdfs:comment": "Flash"
          },
		  {
			"@type": "s3n:Memory",
            "schema:value": 128,
            "schema:unitCode": "om:kilobyte",
			"rdfs:comment": "RAM"
		  }
        }
      ],
      "ssn:implements": [
        {
          "@type": "s3n:Algorithm",
          "rdfs:comment": "Autoencoder for detecting anomaly in vibration on a motor.",
          "s3n:hasProcedureFeature": {
            "ssn-system:inCondition": {
              "@type": "s3n:Memory",
              "rdfs:comment": "Flash requirement.",
              "schema:minValue": "18.5",
              "schema:unitCode": "om:kilobyte"
            },
			"ssn-system:inCondition": {
              "@type": "s3n:Memory",
              "rdfs:comment": "RAM requirement.",
              "schema:minValue": "8.7",
              "schema:unitCode": "om:kilobyte"
            }
          }
        }
      ]
    },
    {
      "@type": "demo:VibrationSensor"
    },
    {
      "@type": "demo:TemperatureSensor"
    }
  ],
  "ssn:isHostedBy": "demo:FESTO_Workstation",
  "securityDefinitions": {
    "nosec_sc": {
      "scheme": "nosec"
    }
  },
  "security": "nosec_sc",
  "properties": {
    "ConveyorBeltStatus": {
      "title": "ConveyorBeltStatus",
      "observable": true,
      "readOnly": true,
      "description": "Status of the conveyor belt on FESTO workstation based on a vibration sensor.",
      "type": "integer",
      "forms": [
        {
          "op": [
            "readproperty",
            "observeproperty"
          ],
          "href": "http://ureasoner.org/tinyml-demo/conveyor-belt-status"
        }
      ]
    },
    "Vibration": {
      "title": "Vibration",
      "observable": true,
      "readOnly": true,
      "description": "Vibration data.",
      "type": "number",
      "maximum": 10,
      "unit": "om:kilohertz",
      "forms": [
        {
          "op": [
            "readproperty",
            "observeproperty"
          ],
          "href": "http://ureasoner.org/tinyml-demo/vibration"
        }
      ]
    },
    "Temperature": {
      "title": "Temperature",
      "observable": true,
      "readOnly": true,
      "description": "Temperature data.",
      "type": "number",
      "minimum": -30,
      "maximum": 60,
      "unit": "om:degree_Celsius",
      "forms": [
        {
          "op": [
            "readproperty",
            "observeproperty"
          ],
          "href": "http://ureasoner.org/tinyml-demo/temperature"
        }
      ]
    }
  },
  "actions": {},
  "events": {},
  "id": "SSI_Web-322-000-000-001",
  "forms": []
}