# baseURI: http://ureasoner.org/tinyml-demo
# imports: http://ureasoner.org/networkschema
# imports: http://w3id.org/s3n/S3NCore
# imports: http://w3id.org/s3n/S3NProcedure
# imports: http://w3id.org/s3n/S3NThing
# imports: http://www.ontology-of-units-of-measure.org/resource/om-2/
# imports: http://w3id.org/s3n/

@prefix : <http://ureasoner.org/tinyml-demo#> .
@prefix nnet: <http://ureasoner.org/networkschema#> .
@prefix om: <http://www.ontology-of-units-of-measure.org/resource/om-2/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix s3n: <http://w3id.org/s3n/> .
@prefix schema: <https://schema.org/> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix ssn: <http://www.w3.org/ns/ssn/> .
@prefix ssn-system: <http://www.w3.org/ns/ssn/systems/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://ureasoner.org/tinyml-demo>
  rdf:type owl:Ontology ;
  owl:imports <http://ureasoner.org/networkschema> ;
  owl:imports <http://w3id.org/s3n/S3NCore> ;
  owl:imports <http://w3id.org/s3n/S3NProcedure> ;
  owl:imports <http://w3id.org/s3n/S3NThing> ;
  owl:imports om: ;
  owl:imports s3n: ;
  owl:versionInfo "Created with TopBraid Composer" ;
.

# Sensor
:Thermometer
  rdf:type owl:Class ;
  rdfs:label "Thermometer" ;
  rdfs:subClassOf sosa:Sensor ;
.
:Accelerometer
  rdf:type owl:Class ;
  rdfs:label "Accelerometer" ;
  rdfs:subClassOf sosa:Sensor ;
.
:Camera
  rdf:type owl:Class ;
  rdfs:label "Camera" ;
  rdfs:subClassOf sosa:Sensor ;
.
:Gyroscope
  rdf:type owl:Class ;
  rdfs:label "Gyroscopes" ;
  rdfs:subClassOf sosa:Sensor ;
.
:Magnetometer
  rdf:type owl:Class ;
  rdfs:label "Magnetometer" ;
  rdfs:subClassOf sosa:Sensor ;
.
:Microphone
  rdf:type owl:Class ;
  rdfs:label "Microphone" ;
  rdfs:subClassOf sosa:Sensor ;
.


# Sensor Observation
:temperature
  rdf:type sosa:ObservableProperty ;
  rdfs:label "temperature" ;
.
:acceleration
  rdf:type sosa:ObservableProperty ;
  rdfs:label "acceleration" ;
.
:image
  rdf:type sosa:ObservableProperty ;
  rdfs:label "image" ;
.
:orientation
  rdf:type sosa:ObservableProperty ;
  rdfs:label "orientation" ;
.
:magneticField
  rdf:type sosa:ObservableProperty ;
  rdfs:label "magnetic field" ;
.
:sound
  rdf:type sosa:ObservableProperty ;
  rdfs:label "sound" ;
.

# Sensor Data
:TemperatureData
  rdf:type s3n:AlgorithmExecution ;
  rdfs:label "TemperatureData" ;
.
:AccelerationData
  rdf:type s3n:AlgorithmExecution ;
  rdfs:label "accelerationData" ;
.
:ImageData
  rdf:type s3n:AlgorithmExecution ;
  rdfs:label "ImageData" ;
.
:OrientationData
  rdf:type s3n:AlgorithmExecution ;
  rdfs:label "OrientationData" ;
.
:MagneticFieldData
  rdf:type s3n:AlgorithmExecution ;
  rdfs:label "MagneticFieldData" ;
.
:SoundData
  rdf:type s3n:AlgorithmExecution ;
  rdfs:label "SoundData" ;
.

# Memory
:RAM
  rdf:type owl:Class ;
  rdfs:label "RAM" ;
  rdfs:subClassOf s3n:Memory ;
.
:Flash
  rdf:type owl:Class ;
  rdfs:label "Flash" ;
  rdfs:subClassOf s3n:Memory ;
.



